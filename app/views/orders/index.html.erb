<h2>All Orders</h2> <br>
<div class="new-order">
    <% @orders.each do |order| %>
    <p>Name: <%= order.name %></p>
    <p>Email: <%= order.email %></p>
    <p>Address: <%= order.address %> <br> <%= order.city%> <br> <%= order.state %> <br> <%= order.zip_code%> </p>
    <p>Line Items: </p>
    <ul>
      <% order.line_items.each do |item| %>
        <li><%= item.quantity %> x <%= item.product.name %></li>
      <% end %>
    </ul>
  <% end %>
</div>
